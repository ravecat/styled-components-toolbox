{
  "extends": [
    "react-app",
    "prettier",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "typescript",
    "import",
    "jsx-a11y"
  ],
  "parser": "typescript-eslint-parser",
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "document": true,
    "window": true,
    "navigator": true,
    "it": true,
    "jest": true,
    "expect": true,
    "describe": true
  },
  "overrides": {
    "files": ["**/*.ts", "**/*.tsx"],
    "parser": "typescript-eslint-parser",
    "rules": {
      "no-undef": "off"
    }
  }
}
