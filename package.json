{
  "name": "styled-components-toolbox",
  "version": "0.5.0",
  "description": "Set of components based on styled-components",
  "homepage": "https://gitlab.com/rvct/styled-components-toolbox",
  "keywords": [
    "css",
    "css-in-js",
    "styled-components",
    "styling",
    "react",
    "react-component",
    "component",
    "components",
    "ui"
  ],
  "author": "Max Sharov",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/rvct/styled-components-toolbox.git"
  },
  "bugs": {
    "url": "https://gitlab.com/rvct/styled-components-toolbox/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@storybook/theming": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "react-test-renderer": "^17.0.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "styled-components": ">= 5"
  },
  "scripts": {
    "storybook": "start-storybook -p 9000",
    "storybook:build": "build-storybook -o ./build",
    "prebuild": "rimraf dist",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "build": "NODE_ENV=production rollup -c",
    "test": "jest --passWithNoTests",
    "lint:eslint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:stylelint": "stylelint '**/*.{js,jsx,ts,tsx,css}'",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:stylelint",
    "release": "semantic-release",
    "release:debug": "semantic-release --debug",
    "prettier": "prettier --check *.{css,md,yml,json}"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged -v && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": "yarn lint:fix",
    "*.{md,yml,json}": "yarn prettier --write"
  },
  "config": {
    "cz-customizable": {
      "config": "./commitizen.config.js"
    }
  }
}
